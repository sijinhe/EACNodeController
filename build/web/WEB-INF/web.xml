<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Bootstrap</servlet-name>
        <servlet-class>com.eac.node.servlet.Bootstrap</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>DownloadAPP</servlet-name>
        <servlet-class>com.eac.node.servlet.DownloadAPP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CleanUp</servlet-name>
        <servlet-class>com.eac.node.servlet.CleanUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLogging</servlet-name>
        <servlet-class>com.eac.node.servlet.GetLogging</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Bootstrap</servlet-name>
        <url-pattern>/Bootstrap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadAPP</servlet-name>
        <url-pattern>/DownloadAPP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CleanUp</servlet-name>
        <url-pattern>/CleanUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLogging</servlet-name>
        <url-pattern>/GetLogging</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
